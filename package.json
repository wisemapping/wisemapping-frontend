{
  "name": "wisemapping-front-end",
  "version": "6.0.1",
  "private": true,
  "homepage": "http://localhost:8080/react",
  "license": "https://wisemapping.atlassian.net/wiki/spaces/WS/pages/524357/WiseMapping+Public+License+Version+1.0+WPL",
  "author": {
    "name": "Paulo Veiga",
    "login.email": "pveiga@wisemapping.com"
  },
  "contributors": [
    "Ezequiel Bergamaschi"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "lerna run build",
    "clean": "lerna clean && rm -rf node_modules",
    "lint": "lerna run lint --stream",
    "lint:fix": "lerna run lint:fix --stream",
    "playground": "lerna run playground --stream",
    "prepare": "husky install",
    "test": "lerna run test --stream",
    "test:integration": "lerna run test:integration --stream",
    "test:unit": "lerna run test:unit --stream"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint && yarn test"
    }
  },
  "lint-staged": {
    "packages/mindplot/src/**/*.{ts,tsx}": [
      "cd packages/mindplot && eslint --fix",
      "prettier --write"
    ],
    "packages/editor/src/**/*.{ts,tsx}": [
      "cd packages/editor && eslint --fix",
      "prettier --write"
    ],
    "packages/webapp/src/**/*.{ts,tsx}": [
      "cd packages/webapp && eslint --fix",
      "prettier --write"
    ],
    "packages/web2d/src/**/*.{ts,tsx}": [
      "cd packages/web2d && eslint --fix",
      "prettier --write"
    ],
    "**/*.{js,jsx}": [
      "prettier --write"
    ],
    "**/*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/register": "^7.28.3",
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@formatjs/cli": "^6.7.2",
    "@storybook/builder-webpack5": "9.1.8",
    "@storybook/react-webpack5": "9.1.10",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-standard": "^17.1.0",
    "eslint-nibble": "^9.0.0",
    "eslint-plugin-cypress": "^5.1.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.4.0",
    "html-webpack-plugin": "^5.6.4",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-diff": "^30.1.2",
    "jest-environment-jsdom": "^30.1.2",
    "jest-webpack": "^0.5.1",
    "lerna": "^8.2.4",
    "lint-staged": "^16.2.1",
    "prettier": "^3.6.2",
    "start-server-and-test": "^2.1.2",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "update-browserslist-db": "^1.1.3",
    "webpack": "^5.101.3",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
