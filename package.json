{
  "name": "wisemapping-front-end",
  "version": "6.0.1",
  "private": true,
  "homepage": "http://localhost:8080/react",
  "license": "http://www.wisemapping.org/license",
  "author": {
    "name": "Paulo Veiga",
    "login.email": "pveiga@wisemapping.com"
  },
  "contributors": [
    "Ezequiel Bergamaschi"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "lerna run build",
    "clean": "lerna run clean --parallel && lerna clean --yes && rm -rf node_modules && yarn install",
    "lint": "lerna run lint --stream",
    "lint:fix": "lerna run lint:fix --stream",
    "playground": "lerna run playground --stream",
    "prepare": "husky install",
    "test": "lerna run test --stream",
    "test:integration": "lerna run test:integration --stream",
    "test:unit": "lerna run test:unit --stream"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint && yarn test"
    }
  },
  "lint-staged": {
    "packages/mindplot/src/**/*.{ts,tsx}": [
      "cd packages/mindplot && eslint --fix",
      "prettier --write"
    ],
    "packages/editor/src/**/*.{ts,tsx}": [
      "cd packages/editor && eslint --fix",
      "prettier --write"
    ],
    "packages/webapp/src/**/*.{ts,tsx}": [
      "cd packages/webapp && eslint --fix",
      "prettier --write"
    ],
    "packages/web2d/src/**/*.{ts,tsx}": [
      "cd packages/web2d && eslint --fix",
      "prettier --write"
    ],
    "**/*.{js,jsx}": [
      "prettier --write"
    ],
    "**/*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      "last 2 chrome versions",
      "last 2 edge versions",
      "last 2 firefox versions",
      "last 2 safari versions"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "defaults": [
      "last 2 chrome versions",
      "last 2 edge versions",
      "last 2 firefox versions",
      "last 2 safari versions"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@babel/register": "^7.28.3",
    "@eslint/compat": "^1.4.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@formatjs/cli": "^6.7.4",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-standard": "^17.1.0",
    "eslint-nibble": "^9.1.1",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-diff": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lerna": "^9.0.3",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "start-server-and-test": "^2.1.3",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "update-browserslist-db": "^1.2.2",
    "vite-plugin-html": "^3.2.2",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
