                }
            ]
        },
        {
            "source": "/:imageAsset*.svg",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, s-maxage=31536000, immutable"
                }
            ]
        },
        {
            "source": "/:imageAsset*.ico",
            "headers": [
                {
