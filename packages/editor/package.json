{
  "name": "@wisemapping/editor",
  "version": "6.0.1",
  "homepage": "https://www.wisemapping.com/opensource/",
  "repository": {
    "type": "git",
    "url": "git@github.com:wisemapping/wisemapping-frontend.git"
  },
  "main": "src/index.ts",
  "license": "MIT",
  "author": "Paulo Veiga <pveiga@wisemapping.com>",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "build-storybook": "STORYBOOK_DISABLE_TELEMETRY=1 storybook build",
    "clean": "rm -rf dist node_modules/.cache storybook-static cypress/screenshots cypress/videos cypress/downloads debug-storybook.log .eslintcache",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:storybook:open": "cypress open --config-file cypress.storybook.config.ts",
    "cy:storybook:run": "cypress run --config-file cypress.storybook.config.ts --browser electron",
    "i18n:compile": "bash -c 'for lang in {'es','en','fr','de','zh','zh-CN','ru','uk','ja','pt','it','hi'};do formatjs compile lang/${lang}.json --ast --out-file src/compiled-lang/${lang}.json;done'",
    "i18n:extract": "formatjs extract 'src/**/*.ts*' --ignore 'src/@types/**/*' --out-file lang/en.json",
    "lint": "eslint src --ext js,ts,tsx",
    "lint:fix": "eslint src --ext js,ts,tsx --fix",
    "playground": "vite",
    "storybook": "STORYBOOK_DISABLE_TELEMETRY=1 storybook dev -p ${PORT:-6008} --quiet",
    "test": "yarn test:unit && yarn test:integration",
    "test:integration": "yarn test:integration:playground && yarn test:integration:storybook",
    "test:integration:playground": "rm -rf node_modules/.vite && PORT=8081 CYPRESS_BASE_URL=http://localhost:8081 npx start-server-and-test 'vite --port 8081' http-get://localhost:8081 cy:run",
    "test:integration:storybook": "PORT=6008 CYPRESS_BASE_URL=http://localhost:6008 STORYBOOK_DISABLE_TELEMETRY=1 npx start-server-and-test 'storybook dev -p 6008 --quiet --ci' http-get://localhost:6008 cy:storybook:run",
    "test:unit": "jest ./test/unit/* --detectOpenHandles"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@wisemapping/mindplot": "workspace:*",
    "emoji-picker-react": "^4.16.1",
    "react-color": "^2.19.3",
    "react-intl": "^7.1.14",
    "react-loader-spinner": "^8.0.0",
    "styled-components": "^6.1.19"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@formatjs/cli": "^6.7.4",
    "@mdx-js/react": "^3.1.1",
    "@storybook/addon-docs": "^10.1.5",
    "@storybook/addon-links": "^10.1.5",
    "@storybook/react-vite": "^10.1.5",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/react": "^16.3.0",
    "@types/cypress-image-snapshot": "^3.1.9",
    "@types/jest": "^30.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-jest": "^30.2.0",
    "babel-polyfill": "^6.26.0",
    "cypress": "^15.7.1",
    "cypress-image-snapshot": "^4.0.1",
    "eslint": "^9.39.1",
    "eslint-plugin-storybook": "^0.11.6",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-transform-stub": "^2.0.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "start-server-and-test": "^2.1.3",
    "storybook": "^10.1.5",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
