{
  "name": "@wisemapping/editor",
  "version": "6.0.1",
  "homepage": "https://www.wisemapping.com/opensource/",
  "repository": {
    "type": "git",
    "url": "git@github.com:wisemapping/wisemapping-frontend.git"
  },
  "main": "src/index.ts",
  "license": "MIT",
  "author": "Paulo Veiga <pveiga@wisemapping.com>",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "storybook": "storybook dev -p 6008 --quiet",
    "build-storybook": "storybook build",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:storybook:open": "cypress open --config-file cypress.storybook.config.ts",
    "cy:storybook:run": "cypress run --config-file cypress.storybook.config.ts --browser electron",
    "i18n:compile": "bash -c 'for lang in {'es','en','fr','de','zh','zh-CN','ru','uk','ja','pt','it','hi'};do formatjs compile lang/${lang}.json --ast --out-file src/compiled-lang/${lang}.json;done'",
    "i18n:extract": "formatjs extract 'src/**/*.ts*' --ignore 'src/@types/**/*' --out-file lang/en.json",
    "lint": "eslint src --ext js,ts,tsx",
    "lint:fix": "eslint src --ext js,ts,tsx --fix",
    "playground": "webpack serve --config webpack.playground.js",
    "test": "yarn test:unit && yarn test:integration",
    "test:integration": "yarn test:integration:playground && yarn test:integration:storybook",
    "test:integration:playground": "npx start-server-and-test 'yarn playground' http-get://localhost:8081 'yarn cy:run'",
    "test:integration:storybook": "npx start-server-and-test 'yarn storybook' http-get://localhost:6008 'yarn cy:storybook:run'",
    "test:unit": "jest ./test/unit/* --detectOpenHandles"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@wisemapping/core-js": "workspace:*",
    "@wisemapping/mindplot": "workspace:*",
    "emoji-picker-react": "^4.13.3",
    "react-color": "^2.19.3",
    "react-intl": "^7.1.11",
    "react-loader-spinner": "^6.1.6",
    "styled-components": "^6.1.19"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@formatjs/cli": "^6.7.2",
    "@mdx-js/react": "^3.1.1",
    "@storybook/addon-links": "^9.1.8",
    "@storybook/builder-webpack5": "^9.1.8",
    "@storybook/react-webpack5": "^9.1.10",
    "@storybook/test": "8.6.14",
    "@testing-library/react": "^16.3.0",
    "@types/cypress-image-snapshot": "^3.1.9",
    "babel-polyfill": "^6.26.0",
    "copy-webpack-plugin": "^13.0.1",
    "cypress": "^15.2.0",
    "cypress-image-snapshot": "^4.0.1",
    "eslint": "^9.36.0",
    "eslint-plugin-storybook": "9.1.8",
    "html-webpack-plugin": "^5.6.4",
    "jest-transform-stub": "^2.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "start-server-and-test": "^2.1.2",
    "storybook": "9.1.8",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "vite": "^7.1.7",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  }
}
