{
    "name": "@wisemapping/webapp",
    "version": "6.0.1",
    "homepage": "https://www.wisemapping.com/opensource/",
    "repository": {
        "type": "git",
        "url": "git@github.com:wisemapping/wisemapping-frontend.git"
    },
    "license": "MIT",
    "author": "Paulo Veiga <pveiga@wisemapping.com>",
    "main": "app.jsx",
    "files": [
        "src"
    ],
    "scripts": {
        "build": "webpack --config webpack.prod.js",
        "build:verbose": "webpack --config webpack.prod.js --stats=verbose",
        "build:analyze": "ANALYZE=true webpack --config webpack.prod.js",
        "build:sizes": "yarn build && ls -lah dist/*.bundle.js | awk '{print $5 \"\\t\" $9}' | sort -hr && ls -la dist/*.bundle.js | awk '{sum += $5} END {print \"Total: \" sum/1024/1024 \" MB\"}'",
        "cy:open": "npx cypress open",
        "cy:run": "npx cypress run",
        "dev": "webpack --config webpack.dev.js",
        "i18n:compile": "bash -c 'for lang in {'es','en','fr','de','zh','zh-CN','ru','uk','ja','pt','it','hi'};do formatjs compile lang/${lang}.json --ast --out-file src/compiled-lang/${lang}.json;done'",
        "i18n:extract": "formatjs extract 'src/**/*.ts*' --ignore 'src/@types/**/*' --out-file lang/en.json",
        "lint": "eslint src --ext js,ts,tsx",
        "lint:fix": "eslint src --ext js,ts,tsx --fix",
        "start": "webpack serve --config webpack.dev.js ",
        "storybook": "storybook dev -p ${PORT:-6007} --quiet",
        "build-storybook": "storybook build",
        "test": "yarn test:integration",
        "test:integration": "PORT=3000 CYPRESS_BASE_URL=http://localhost:3000 npx start-server-and-test 'webpack serve --config webpack.dev.js --port 3000' http-get://localhost:3000 cy:run",
        "start:dev": "APP_CONFIG_TYPE='file:dev' webpack serve --config webpack.dev.js "
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/icons-material": "^7.3.2",
        "@mui/lab": "^7.0.0-beta.17",
        "@mui/material": "^7.3.4",
        "@wisemapping/editor": "workspace:^",
        "axios": "^1.12.2",
        "dayjs": "^1.11.18",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-ga4": "^2.1.0",
        "react-google-recaptcha": "^3.1.0",
        "react-helmet-async": "^2.0.5",
        "react-intl": "^7.1.14",
        "react-query": "^3.39.3",
        "react-router-dom": "^7.0.0",
        "styled-components": "^6.1.19",
        "universal-cookie": "^8.0.1",
        "update-browserslist-db": "^1.1.3"
    },
    "devDependencies": {
        "@formatjs/cli": "^6.7.4",
        "@mdx-js/react": "^3.1.1",
        "@storybook/addon-docs": "^9.1.10",
        "@storybook/addon-links": "^9.1.10",
        "@storybook/react-vite": "^9.1.10",
        "@testing-library/cypress": "^10.1.0",
        "@welldone-software/why-did-you-render": "^10.0.1",
        "cypress": "^15.2.0",
        "cypress-image-snapshot": "^4.0.1",
        "eslint": "^9.36.0",
        "eslint-plugin-storybook": "9.1.10",
        "storybook": "^9.1.10",
        "vite": "^7.1.7"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.com"
    }
}
