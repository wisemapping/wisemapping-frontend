                <topic position="-470,-12" order="1" text="Use it" shape="line" id="1088353959">
                    <topic position="470,-24" order="0" text="Installing FreeMind applet at your web site" shape="line" shrink="true" id="1525986009">
                        <topic position="-470,-74" order="0" text="You can install the applet at your website so that other users can browse your mind maps." shape="line" id="1369857212"/>
