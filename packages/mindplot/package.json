{
  "name": "@wisemapping/mindplot",
  "version": "6.0.1",
  "description": "WiseMapping - Mindplot Canvas Library",
  "homepage": "https://www.wisemapping.com/opensource/",
  "repository": {
    "type": "git",
    "url": "git@github.com:wisemapping/wisemapping-frontend.git"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "author": "Paulo Veiga <pveiga@wisemapping.com>",
  "files": [
    "dist",
    "assets",
    "vendor"
  ],
  "scripts": {
    "build": "vite build",
    "build-storybook": "storybook build",
    "clean": "rm -rf dist node_modules/.cache storybook-static cypress/screenshots cypress/videos debug-storybook.log .eslintcache",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "vite build --watch",
    "lint": "eslint src --ext js,ts",
    "lint:fix": "eslint src --ext js,ts --fix",
    "storybook": "storybook dev -p ${PORT:-6006} --quiet",
    "test": "yarn test:unit && yarn test:integration",
    "test:integration": "PORT=6006 CYPRESS_BASE_URL=http://localhost:6006 npx start-server-and-test 'storybook dev -p 6006 --no-open --quiet' http-get://localhost:6006 cy:run",
    "test:unit": "jest ./test/unit/**/*.test.ts --verbose --silent --detectOpenHandles"
  },
  "dependencies": {
    "@wisemapping/web2d": "workspace:*",
    "fflate": "file:vendor/fflate",
    "html2canvas": "^1.4.1",
    "jspdf": "^4.2.0",
    "lodash": "^4.17.23",
    "xml-formatter": "^3.6.7"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-typescript": "^7.28.5",
    "@mdx-js/react": "^3.1.1",
    "@storybook/addon-docs": "^10.2.10",
    "@storybook/addon-links": "^10.2.10",
    "@storybook/html-vite": "^10.2.10",
    "@storybook/testing-library": "^0.2.2",
    "blob-polyfill": "^9.0.20240710",
    "cypress": "^15.10.0",
    "cypress-image-snapshot": "^4.0.1",
    "eslint": "^10.0.1",
    "eslint-plugin-storybook": "^10.2.10",
    "jest": "^30.2.0",
    "mocha": "^11.7.5",
    "raphael": "^2.3.0",
    "storybook": "^10.2.10",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com"
  }
}
