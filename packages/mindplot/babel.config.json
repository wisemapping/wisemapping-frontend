{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": false,
                "targets": {
                    "esmodules": true
                }
            },
            "@babel/preset-typescript"
        ]
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties"
    ],
    "sourceType": "module"
}